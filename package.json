{
  "name": "antd-simple-table",
  "version": "0.0.0-development",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --emitDeclarationOnly && babel src --out-dir dist --extensions \".ts,.tsx\"",
    "dev": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"stories/**/*.ts\"",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "prepublish": "npm run build",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadowolfapp/antd-simple-table.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shadowolfapp/antd-simple-table/issues"
  },
  "homepage": "https://github.com/shadowolfapp/antd-simple-table#readme",
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "antd": "^4.2.4",
    "dnd-core": "^10.0.2",
    "file-saver": "^2.0.2",
    "immutability-helper": "^3.0.2",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "papaparse": "^5.2.0",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-resizable": "^1.10.1",
    "react-use": "^15.1.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-options": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@types/file-saver": "^2.0.1",
    "@types/lodash": "^4.14.152",
    "@types/papaparse": "^5.0.3",
    "@types/react": "^16.9.34",
    "@types/react-resizable": "^1.7.2",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.1.2",
    "css-loader": "^3.5.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-config-standard-with-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "semantic-release": "^17.0.7",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
